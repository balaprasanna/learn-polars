    "#     storage_options=storage_options_s3\n",
    "# )\n",
    "\n",
    "# Read Parquet from S3 (lazy for better performance)\n",
    "# df = pl.scan_parquet(\n",
    "#     's3://my-bucket/path/to/data.parquet',\n",
    "#     storage_options=storage_options_s3\n",
    "# ).collect()\n",
    "\n",
    "print(\"Example: Reading from S3 with explicit credentials\")"
