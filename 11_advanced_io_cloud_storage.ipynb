   "source": [
    "## 4.1 Excel Files"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Writing to Excel\n",
    "df_excel = pl.DataFrame({\n",
    "    'Product': ['Laptop', 'Mouse', 'Keyboard', 'Monitor'],\n",
    "    'Price': [1200, 25, 75, 300],\n",
    "    'Stock': [15, 100, 50, 30],\n",
    "    'Category': ['Electronics', 'Accessories', 'Accessories', 'Electronics']\n",
